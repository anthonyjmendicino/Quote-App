<p id="notice"><%= notice %></p>
<div class="w3-container w3-margin"
<h1>Typos</h1>

<table class="w3-table-all">
  <thead>
    <tr>
      <th>Name</th>
      <th>Stuff</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @typos.each do |typo| %>
      <tr>
        <td><%= typo.name %></td>
        <td><%= typo.stuff %></td>
        <td><%= link_to 'Show', typo , class:"w3-button w3-red" %></td>
        <td><%= link_to 'Edit', edit_typo_path(typo) , class:"w3-button w3-red" %></td>
        <td><%= link_to 'Destroy', typo, method: :delete, data: { confirm: 'Are you sure?' } , class:"w3-button w3-red" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Typo', new_typo_path , class:"w3-button w3-red" %>
</div>