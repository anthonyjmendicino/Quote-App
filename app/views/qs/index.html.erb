<p id="notice"><%= notice %></p>
<div class="w3-container w3-margin"
<h1>Qs</h1>

<table class="w3-table-all">
  <thead>
    <tr>
      <th>One</th>
      <th>Two</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @qs.each do |q| %>
      <tr>
        <td><%= q.one %></td>
        <td><%= q.two %></td>
        <td><%= link_to 'Show', q , class:"w3-button w3-red" %></td>
        <td><%= link_to 'Edit', edit_q_path(q) , class:"w3-button w3-red" %></td>
        <td><%= link_to 'Destroy', q, method: :delete, data: { confirm: 'Are you sure?' } , class:"w3-button w3-red" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Q', new_q_path , class:"w3-button w3-red" %>
</div>